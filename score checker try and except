try:
    score = float(input('enter score: '))
except ValueError:
    print('bad score')
    quit()
if score >= 0.0 and score <=1.0 :
    if score>=0.9:
        print('A')
    elif score>=0.8:
        print('B')
    elif score>=0.7:
        print('C')
    elif score<0.6:
        print('D')
    else:
        print('F')
else:
    print("bad score")

inhf =input('hfjhgkhjh')
inhf = bool(inhf)
print(f'{inhf}')
